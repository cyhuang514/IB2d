}


%
% CONCENTRATION MODEL INFORMATION %
%
Concentration_Info {
concentration = 1     % 0 (for no) or 1 (for yes)
kDiff = 0.01          % Diffusion coefficient
advection = 0         % 0 (1st order upwind) or 1 (3rd order WENO)
source = 1            % 0 (for no) or 1 (for yes)
}


% PRINTING INFORMATION
%
Output_Info {
print_dump = 1           % How often to print (PLOT) to screen
save_Vorticity = 1       % Save Vorticity Scalar Data to .vtk format
save_Pressure = 1        % Save Pressure Scalar Data to .vtk format
